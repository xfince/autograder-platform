{
  "name": "autograder-platform",
  "version": "1.0.0",
  "private": true,
  "packageManager": "npm@10.0.0",
  "description": "AutoGrader Platform - Real-time grading system for student projects",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "dependencies": {
    "axios": "^1.6.0",
    "openai": "^4.20.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "nodemailer": "^6.9.7",
    "playwright": "^1.40.0",
    "socket.io": "^4.6.0",
    "supertest": "^6.3.3",
    "turbo": "^2.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
