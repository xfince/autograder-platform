{
  "name": "workers",
  "version": "0.0.1",
  "description": "AutoGrader Platform - Background Workers for Grading Jobs",
  "private": true,
  "packageManager": "npm@10.0.0",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "start": "node dist/main.js",
    "lint": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest"
  },
  "dependencies": {
    "bull": "^4.12.0",
    "dotenv": "^16.3.1",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@types/bcryptjs": "^3.0.0",
    "@types/bull": "^4.10.0",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  }
}
