# Database (using Docker service name)
DATABASE_URL="postgresql://autograder:autograder_dev_password@postgres:5432/autograder_dev?schema=public"

# API
PORT=3001
NODE_ENV=production

# Frontend (using Docker service name)
FRONTEND_URL=http://web:3000

# JWT Authentication
JWT_SECRET=dev-jwt-secret-change-in-production-please-use-256-bit-key
JWT_EXPIRES_IN=7d
JWT_REFRESH_SECRET=dev-refresh-secret-change-in-production-please-use-256-bit-key
JWT_REFRESH_EXPIRES_IN=30d

# Redis (using Docker service name)
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=autograder_redis_password

# Bull MQ
QUEUE_NAME_PREFIX=autograder
CONCURRENT_JOBS=2

# OpenAI API (for GPT evaluation)
OPENAI_API_KEY=your-openai-api-key-here

# LangSmith (AI monitoring)
LANGSMITH_API_KEY=your-langsmith-api-key-here
LANGSMITH_PROJECT=autograder-dev

# AWS S3 (for production)
AWS_REGION=us-east-1
AWS_S3_BUCKET=autograder-artifacts
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key

# File Upload
MAX_FILE_SIZE=10485760

# Rate Limiting
RATE_LIMIT_TTL=60
RATE_LIMIT_MAX=100
